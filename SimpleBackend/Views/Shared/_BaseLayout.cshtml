<!DOCTYPE html>
<html lang="en">

<head>
    @RenderPage(MVC.Shared.Views.LayoutPage.Head)

    @RenderSection("styles", required: false)

    <!-- 標題在Head.cshtml設定 -->
    <title></title>

</head>

<body>

    @RenderBody()

    <!-- jQuery -->
    @Scripts.Render(ScriptName.Bundle.JQuery)

    <!-- Bootstrap Core JavaScript -->
    @Scripts.Render(ScriptName.Bundle.Bootstrap)

    <!-- Metis Menu Plugin JavaScript, Custom Theme JavaScript -->
    @Scripts.Render(ScriptName.Bundle.SbAdmin2)

    @Scripts.Render(ScriptName.Bundle.ETdea)

    @RenderSection("scripts", required: false)

</body>

</html>